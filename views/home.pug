    script.
      $(function(){
        $(".main").on("change", function(){
          $(".models.active").removeClass("active");
          var List = $(".models."+$(this).val());
          if (List.length){
            List.addClass("active");
            $(".models").find(".form-control").removeAttr('name');
            $(".models").find(".form-control").removeAttr('required');
            $(List).find(".form-control").attr('name', 'subCategory');
            $(List).find(".form-control").attr('required', true);
          }
        });
      });
    script.
      $(function(){
        $(".state").on("change", function(){
          $(".states.active").removeClass("active");
          var subList = $(".states."+$(this).val());
          if (subList.length){
            subList.addClass("active");
            $(".states").find(".form-control").removeAttr('name');
            $(".states").find(".form-control").removeAttr('required');
            $(subList).find(".form-control").attr('name', 'district');
            $(subList).find(".form-control").attr('required', true);
          }
        });
      });