    script.
      $('.del_val').click(function(){
        var id = $(this).val();
        $.post('/remove',{id:id}, function(data,textStatus,jqXHR){
          alert('Deleted Successfully')
          location.reload();
        });
      });
    script.
      $('.edit_val').click(function(){
        var id = $(this).val();
        $.post('/edit',{id:id}, function(data,textStatus,jqXHR){
          var jsondata = JSON.stringify(data[0]);
            var docs = JSON.parse(jsondata);
            $("#e").val(docs.Email);
            $("#n").val(docs.Name);
            $("#r").val(docs.Role);
            $("#m").val(docs.Mandal);
            $("#v").val(docs.Village);
            $("#sc").val(docs.School);
            $("#i").val(docs.Id);
            $("#s").val(docs.State);
            $("#c").val(docs.Country);
            $("#d").val(docs.District);
        });
        $('.form').show();
      });
    script.
      $(function(){
        $(".state").on("change", function(){
          $(".states.active").removeClass("active");
          var subList = $(".states."+$(this).val());
          if (subList.length){
            subList.addClass("active");
            $(".states").find(".form-control").removeAttr('name');
            $(".states").find(".form-control").removeAttr('required');
            $(subList).find(".form-control").attr('name', 'district');
            $(subList).find(".form-control").attr('required', true);
          }
        });
      });